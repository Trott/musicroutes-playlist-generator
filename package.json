    "url": "https://github.com/Trott/musicroutes-playlist-generator/issues"
  },
  "homepage": "https://github.com/Trott/musicroutes-playlist-generator",
  "dependencies": {
    "browserify": "^8.0.2",
    "flat": "^1.3.0",
    "hyperquest": "^1.0.1",
    "jquery": "^2.1.3",
    "lodash": "^2.4.1",
    "mqlread": "^1.1.0",
    "promise": "^6.0.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "cheerio": "^0.18.0",
    "code": "^1.2.1",
    "jshint": "^2.5.10",
    "lab": "^5.1.0",
    "minifyify": "^5.0.0",
