{
  "name": "musicroutes-playlist-generator",
  "version": "1.1.8",
  "description": "musicroutes-playlist-generator",
  "scripts": {
    "test": "lab -t 100 _test",
    "build": "browserify src.js -o bundle.js -d -p bundle-collapser/plugin -p [minifyify --map bundle.map.json --output bundle.map.json] && vulcanize -o index.html src.html",
    "build-ie": "browserify src.js -o bundle.js -d -p bundle-collapser/plugin -p [minifyify --map bundle.map.json --output bundle.map.json] && cp src.html index.html",
    "watch": "watchify --debug src.js -o bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trott/musicroutes-freebase-demo.git"
  },
  "author": "Rich Trott <rtrott@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trott/musicroutes-playlist-generator/issues"
  },
  "homepage": "https://github.com/Trott/musicroutes-playlist-generator",
  "dependencies": {
    "browserify": "^8.0.2",
    "flat": "^1.5.0",
    "hyperquest": "^1.0.1",
    "jquery": "^3.3.1",
    "lodash": "^3.1.0",
    "mqlread": "^1.1.0",
    "promise": "^6.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "bundle-collapser": "^1.1.1",
    "cheerio": "^0.18.0",
    "code": "^5.1.2",
    "jshint": "^2.9.5",
    "lab": "^15.2.0",
    "minifyify": "^5.0.0",
    "nock": "^9.1.6",
    "precommit-hook": "^1.0.7",
    "rewire": "^2.2.0",
    "vulcanize": "^0.7.9"
  }
}
